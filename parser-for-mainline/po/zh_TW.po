msgstr "%s: 偵測到內部緩衝區溢位，超過 %d 個字元\n"

#: ../parser_regex.c:357
#, c-format
msgid "%s: Unable to parse input line '%s'\n"
msgstr "%s: 無法剖析輸入行 '%s'\n"

#: ../parser_regex.c:390
#, c-format
msgid "%s: Failed to compile regex '%s' [original: '%s']\n"
msgstr "%s: 無法編譯 regex '%s' [原始： '%s']\n"

#: ../parser_regex.c:394
#, c-format
msgid "%s: error near               "
msgstr "%s: 錯誤接近"

#: ../parser_regex.c:404
#, c-format
